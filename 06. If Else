public class logical {
        public static void main(String[] args){
    
            int marks = 78;
            char gender ='f';
            if (marks > 75 && gender == 'f'){
                System.out.println("You are selected for operational team."); 
            } 
                    else if (marks > 75 && gender == 'm'){
                System.out.println("You are selected for marketing team."); 
            }
            else{
                System.out.println("You are not selected."); 
            }                 
        }
    }
